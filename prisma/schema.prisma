datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model SplitSchema {
    id String @id @default(cuid())
    createdAt DateTime @default(now())
    name String @db.VarChar(255)
    userId String

    @@index([userId])
}